{
	"name": "parse-ansi",
	"main": "index.js",
	"version": "1.0.3",
	"description": "ðŸ¤–  Parse ANSI text into an array of ansi-tags and text-chunks.",
	"license": "MIT",
	"repository": "F1LT3R/parse-ansi",
	"author": {
		"name": "F1LT3R",
		"email": "al@pwn.io",
		"url": "f1lt3r.io"
	},
	"scripts": {
		"test": "xo && yarn run cover",
		"unit": "ava",
		"watch": "ava --watch",
		"cover": "nyc --reporter=lcov --reporter=html --reporter=text ava *.test.js",
		"coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls -v",
		"precommit": "yarn test"
	},
	"files": [
		"index.js",
		"ansi-seqs-to-ansi-tags.js",
		"ansi-tags-to-decorator-names.js"
	],
	"keywords": [
		"ansi",
		"parse",
		"parser",
		"escape",
		"sequence",
		"chalk",
		"color",
		"open",
		"close",
		"style",
		"character"
	],
	"dependencies": {
		"ansi-regex": "^3.0.0",
		"array-uniq": "^1.0.3",
		"deepmerge": "^2.1.0",
		"strip-ansi": "^4.0.0",
		"super-split": "^1.1.0"
	},
	"devDependencies": {
		"ava": "^0.25.0",
		"chalk": "^2.3.2",
		"coveralls": "^3.0.0",
		"git-hooks": "^1.1.10",
		"git-pre-commit": "^2.1.3",
		"nyc": "^11.6.0",
		"xo": "^0.20.3"
	},
	"nyc": {
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"esnext": true,
		"semicolon": false,
		"globals": [
			"__dirname"
		],
		"rules": {
			"no-console": 1,
			"space-before-function-paren": 0
		}
	},
	"ava": {
		"failFast": true
	}
}
